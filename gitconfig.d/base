[core]
    excludesfile = ~/.gitconfig.d/excludes
    attributesfile = ~/.gitconfig.d/attributes
    compression = -1
    editor = nano
    whitespace = blank-at-eol,blank-at-eof,space-before-tab,tab-in-indent

[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
    ui = true

[svn]
    rmdir = true

[branch]
    autosetuprebase = always

[push]
    default = upstream

[diff]
    renames = copies
    mnemonicprefix = true

[merge]
    stat = true

[http]
    sslverify = false
    postBuffer = 524288000

[credential]
    helper = cache --timeout=18000
